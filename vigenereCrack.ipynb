    "def vigenere(key):\n",
    "    message=\"lucas\"\n",
    "    key=[ord(i)%97 for i in key]\n",
    "    final=\"\"\n",
    "    for i,c in enumerate(message):\n",
    "        final+=chr(ord(c)+key[i%len(key)])\n",
    "    return final"
   ]
